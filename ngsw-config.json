{
  "index": "/index.html",
  "assetGroups": [{
    "name": "app",
    "installMode": "prefetch",
    "resources": {
      "files": [
        "/favicon.ico",
        "/index.html",
        "/*.css",
        "/*.js"
      ]
    }
  }, {
    "name": "assets",
    "installMode": "lazy",
    "updateMode": "prefetch",
    "resources": {
      "files": [
        "/assets/**"
      ]
    }
  }],
  "dataGroups": [{
    "name": "yahoo-weather-api",
    "urls": ["https://query.yahooapis.com/v1/public/yql"],
    "cacheConfig": {
      "maxSize": 3,
      "maxAge": "30m",
      "strategy": "freshness"
    }
  }]
}
